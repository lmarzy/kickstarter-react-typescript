{
  "name": "kickstarter-react-typescript",
  "version": "1.0.0",
  "description": "Kickstarter for a React/Typescript project",
  "main": "index.js",
  "repository": "https://github.com/lmarzy/kickstarter-react-typescript.git",
  "author": "Lee Marsden",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "Jest",
    "start": "webpack-dev-server",
    "build": "webpack"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^20.0.1",
    "awesome-typescript-loader": "^3.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^20.0.6",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "yarn": "^0.24.5"
  },
  "dependencies": {
    "@types/react": "^15.0.29",
    "@types/react-dom": "^15.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
